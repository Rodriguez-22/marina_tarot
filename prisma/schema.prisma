datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Cita {
  id        Int      @id @default(autoincrement())
  nombre    String
  apellidos String
  asunto    String
  fecha     String
  hora      String
  createdAt DateTime @default(now())
}